{
  "name": "yest-5",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "iosSE": "react-native run-ios --simulator=\"iPhone SE\"",
    "ios8": "react-native run-ios --simulator=\"iPhone 8\"",
    "iosMax": "react-native run-ios --simulator=\"iPhone 12 Pro Max\"",
    "ios11": "react-native run-ios --simulator=\"iPhone 11\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && pod install && cd ..",
    "pod2": "cd ios && arch -x86_64 pod install && cd ..",
    "podup": "cd ios && arch -x86_64 pod install --repo-update",
    "rb": "rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "rbs": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "createBundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "createApk": "cd android && ./gradlew assembleRelease",
    "runAndroid": "open -a /Applications/Android\\ Studio\\ 2.app",
    "clear": "npm cache clear --force"
  },
